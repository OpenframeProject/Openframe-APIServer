{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!../client/favicon.ico"
    }
  },
  "initial": {
    "compression": {},
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": []
  },
  "final": {
    "./middleware/url-not-found-handler": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
         "debug": false,
         "log": false
       }
    }
  }
}
